{
  "manifest_version": 2,
  "name": "Audio Overlord",
  "version": "0.0.1",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "16": "volume-high-2x.png",
      "24": "volume-high-2x.png",
      "32": "volume-high-2x.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "storage"
  ]
}